/*
How would you design a stack which, in addition to push and pop, also has a function
min which returns the minimum element? Push, pop and min should all operate in
O(1) time.
__________
*/

public class StackCCode23<Item extends Comparable<Item>> implements iterable<Item> {
  private Comparable min;
  private Comparable max;
  private Node top;
  private int size;

  private class Node {
    private Item data;
    private Node next;
  }

  public StackCCode23() {
    front=null;
    size=0;
  }

  public void push(Item item) {}
  public Item pop() {return null;}
  public Item min() {return min;}
  public Item max() {return max;}
  private boolean isEmpty(){return top=null;}
}
